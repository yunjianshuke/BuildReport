<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yunjian.datarelation.mapper.ETLFunGroupMapper">
  <resultMap id="etlFunGroupMap" type="com.yunjian.datarelation.dto.ETLFunGroupDTO">
    <id property="id" column="id"/>
    <result property="groupName" column="group_name"/>
    <result property="groupEnable" column="group_enable"/>
    <result property="version" column="version"/>
    <result property="createTime" column="create_time"/>
    <result property="createBy" column="create_by"/>
    <result property="updateTime" column="update_time"/>
    <result property="updateBy" column="update_by"/>
    <result property="version" column="version"/>
    <result property="sort" column="sort"/>
  </resultMap>
  <select id="getETLFunGroupList" resultMap="etlFunGroupMap">
    select *
    from report_etl_fun_group
    where group_enable = 1
    order by sort asc
  </select>

</mapper>